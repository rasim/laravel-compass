{
    "name": "rasim/laravel-compass",
    "description": "An elegant REST assistent for the Laravel framework.",
    "keywords": [
        "laravel",
        "API",
        "testing",
        "documentation"
    ],
    "homepage": "https://github.com/davidhsianturi/laravel-compass",
    "license": "MIT",
    "authors": [{
        "name": "David H. Sianturi",
        "email": "davidhsianturi@gmail.com"
    }],
    "require": {
        "php": "^7.3|^8.0|^9.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Davidhsianturi\\Compass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davidhsianturi\\Compass\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Davidhsianturi\\Compass\\CompassServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
